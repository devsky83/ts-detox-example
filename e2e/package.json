{
  "name": "e2e",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "detox": "^9.0.4"
  },
  "scripts": {
    "start": "detox run-server",
    "test": "detox test"
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "jest.config.json",
    "session": {
      "server": "ws://localhost:8099",
      "sessionId": "YourProjectSessionId"
    },
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "../ios/build/Build/Products/Debug-iphonesimulator/TsDetox.app",
        "build": "xcodebuild -project ../ios/TsDetox.xcodeproj -scheme TsDetox -configuration Debug -sdk iphonesimulator -derivedDataPath ../ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    }
  },
  "dependencies": {
    "jest": "^23.6.0"
  }
}
