{
  "name": "e2e",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/detox": "^9.0.0",
    "@types/jasmine": "^2.8.9",
    "@types/jest": "^23.3.5",
    "detox": "^9.0.4",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3"
  },
  "scripts": {
    "start": "detox run-server",
    "test": "detox test"
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "jest.config.json",
    "session": {
      "server": "ws://localhost:8099",
      "sessionId": "YourProjectSessionId"
    },
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "../ios/build/Build/Products/Debug-iphonesimulator/TsDetox.app",
        "build": "xcodebuild -project ../ios/TsDetox.xcodeproj -scheme TsDetox -configuration Debug -sdk iphonesimulator -derivedDataPath ../ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      },
      "android.emu.debug": {
        "binaryPath": "../android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd ../android; ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug; cd ../e2e",
        "type": "android.emulator",
        "name": "Pixel_2_API_27"
      },
      "android.emu.release": {
        "binaryPath": "../android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd ../android; ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release; cd ../e2e",
        "type": "android.emulator",
        "name": "Pixel_2_API_27"
      }
    }
  }
}
